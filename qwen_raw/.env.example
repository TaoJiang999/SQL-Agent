# ============================================
# Proxy Settings
# ============================================
HTTP_PROXY=http://127.0.0.1:7890
HTTPS_PROXY=http://127.0.0.1:7890

# ============================================
# LangChain/LangSmith Settings
# ============================================
LANGCHAIN_API_KEY=your_langchain_key
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=agent-app
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# ============================================
# Default Model Settings
# ============================================
# Choose ONE of the following configurations:

# ---------------------------------------------
# Option 1: Local vLLM Deployment
# Use this if you're running a local vLLM server with Qwen or other models
# Start vLLM with: ./scripts/start_vllm.sh
# ---------------------------------------------
MODEL_NAME=qwen-agent
MODEL_PROVIDER=openai
MODEL_BASE_URL=http://localhost:8000/v1
MODEL_API_KEY=not-needed

# ---------------------------------------------
# Option 2: Cloud API Provider
# Use this if you're using a cloud-based LLM API service
# ---------------------------------------------
# MODEL_NAME=gemini-2.5-flash
# MODEL_PROVIDER=google_genai
# MODEL_BASE_URL=https://api.xstx.info
# MODEL_API_KEY=your_api_key

MODEL_TEMPERATURE=0.7
MODEL_MAX_TOKENS=4096

# ============================================
# Docker Sandbox Settings
# ============================================
DOCKER_SANDBOX_ENABLED=true
DOCKER_SANDBOX_IMAGE=mysql:8.0
DOCKER_SANDBOX_CONTAINER_NAME=sql-agent-sandbox
DOCKER_SANDBOX_NETWORK=sql-agent-network

# Sandbox Database Config (MySQL)
SANDBOX_DB_HOST=localhost
SANDBOX_DB_PORT=3307
SANDBOX_DB_USER=sandbox_user
SANDBOX_DB_PASSWORD=sandbox_password
SANDBOX_DB_NAME=sandbox_db
SANDBOX_DB_ROOT_PASSWORD=root_password

# Sandbox Resource Limits
SANDBOX_MEMORY_LIMIT=512m
SANDBOX_CPU_LIMIT=1.0
SANDBOX_TIMEOUT_SECONDS=30

# ============================================
# Debug Settings
# ============================================
DEBUG=true
DEBUG_AGENT_CALLS=true
DEBUG_LOG_LEVEL=DEBUG
DEBUG_SHOW_PROMPTS=false
DEBUG_SHOW_TOOL_CALLS=true

# ============================================
# SQL Agent Settings
# ============================================
SQL_MAX_RETRIES=3
